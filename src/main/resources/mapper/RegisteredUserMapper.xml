<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.publishing.mapper.RegisteredUserMapper">
    <select id="selectByName" parameterType="String" resultType="com.publishing.entity.RegisteredUser">
        SELECT * FROM registeredUser WHERE username = {#name}
